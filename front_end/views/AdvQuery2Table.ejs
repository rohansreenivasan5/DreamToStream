<style>
    table {
        width: 100%;
        border-collapse: collapse;
        font-family: Arial, sans-serif; 
        margin-top: 20px;
    }

    th, td {
        border: 1px solid #000;
        padding: 8px;
        text-align: left;
    }

    th {
        background-color: #007bff;
        color: #fff;
        font-size: 16px;
        font-weight: bold;
    }

    tr:nth-child(even) {
        background-color: #f8f9fa;
    }

    tr:hover {
        background-color: #e2e6ea;
    }
</style>

<table>
    <tr>
        <th>Movie Name</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Rating</th>
    </tr>
   
    <% 
        if(sampleData.length > 0)
    {
        sampleData.forEach(function(dataQuery2){

    %>

    <tr>
        <td><%= dataQuery2.name %></td>
        <td><%= dataQuery2.firstName %></td>
        <td><%= dataQuery2.lastName %></td>
        <td><%= dataQuery2.value %></td>
    </tr>
    
    <%
        });
    }
    %>

</table>

